{
  "compilerOptions": {
    "outDir": "./dist/win-app",
    "sourceMap": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@routes/*": [
        "src/routes/*"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@public/*": [
        "public/*"
      ],
      
    }
  },
  "exclude": [
    "node_modules"
  ]
}